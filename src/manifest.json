{
  "manifest_version": 3,
  "name": "GitHub Isometric Contributions",
  "version": "1.1.30",
  "description": "Renders an isometric pixel view of GitHub contribution graphs.",
  "content_scripts": [
    {
      "css": ["iso.css"],
      "js": ["obelisk.min.js", "iso.js"],
      "matches": ["https://github.com/*"]
    }
  ],
  "permissions": ["storage"],
  "host_permissions": ["https://github.com/"],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
